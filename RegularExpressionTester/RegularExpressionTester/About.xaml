<Window x:Class="RegularExpressionTester.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="About" Height="250" Width="400" Foreground="Black">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="{DynamicResource StartColor}" Offset="0" />
            <GradientStop Color="{DynamicResource EndColor}" Offset="1" />
        </LinearGradientBrush>
    </Window.Background>
    <Grid>
        <Label Content="Regular Expression Tester" Height="49" HorizontalAlignment="Center" Margin="0,30,0,0" Name="label1" VerticalAlignment="Top" Foreground="Black" FontFamily="Cambria Math" FontSize="30" />
        <Label Content="Created By: Anuj Agrawal" Height="40" HorizontalAlignment="Center" Margin="0,90,0,0" Name="label2" VerticalAlignment="Top" FontSize="20" />
        <Label Content="Website: http://www.anujgeek.com/" FontSize="20" Height="40" HorizontalAlignment="Center" Margin="0,120,0,0" Name="label3" VerticalAlignment="Top" />
        <Button Content="OK" Height="23" HorizontalAlignment="Center" Margin="0,175,0,0" Name="button1" VerticalAlignment="Top" Width="75" IsDefault="True" Click="button1_Click" />
    </Grid>
</Window>
